{
  "extends": [
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaVersion": 12
  },
  "rules": {
    "consistent-return": 0,
    "no-underscore-dangle": ["error", {"allow": ["_id"]}],
    "linebreak-style": 0,
    "no-unused-vars": 0,
    "no-useless-escape": "warn",
    "no-param-reassign":"off"

  }
}
